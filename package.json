{
    "name": "react-jsonschema-inspector",
    "version": "3.1.0-SNAPSHOT",
    "description": "View component for traversing/searching in a JSON Schema",
    "homepage": "https://CarstenWickner.github.io/react-jsonschema-inspector",
    "author": "Carsten Wickner",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/CarstenWickner/react-jsonschema-inspector.git"
    },
    "scripts": {
        "start": "start-storybook -p 9001 -c .storybook",
        "build": "rollup -c",
        "lint": "eslint src stories test && stylelint src stories/**/*.*css stories/**/*.js",
        "test": "jest",
        "predeploy": "rm -rf docs && npm run lint && npm run test --bail && build-storybook -c .storybook -o docs",
        "deploy": "gh-pages -d docs",
        "prepublishOnly": "rm -rf build && npm run lint && npm run test --bail && npm run build"
    },
    "main": "build/index.js",
    "files": [
        "/build"
    ],
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "@babel/cli": "~7.6.3",
        "@babel/core": "~7.6.3",
        "@babel/plugin-proposal-class-properties": "~7.5.5",
        "@babel/preset-env": "~7.6.3",
        "@babel/preset-react": "~7.6.3",
        "@storybook/addon-actions": "~5.2.3",
        "@storybook/addon-docs": "~5.2.3",
        "@storybook/addon-options": "~5.2.3",
        "@storybook/react": "~5.2.3",
        "babel-eslint": "~10.0.3",
        "babel-jest": "~24.9.0",
        "babel-loader": "~8.0.6",
        "coveralls": "~3.0.6",
        "css-loader": "~3.2.0",
        "enzyme": "~3.10.0",
        "enzyme-adapter-react-16": "~1.15.0",
        "enzyme-to-json": "~3.4.2",
        "eslint": "~6.5.1",
        "eslint-config-airbnb": "~18.0.1",
        "eslint-plugin-babel": "~5.3.0",
        "eslint-plugin-import": "~2.18.2",
        "eslint-plugin-jsdoc": "~15.10.0",
        "eslint-plugin-jsx-a11y": "~6.2.3",
        "eslint-plugin-react": "~7.16.0",
        "gh-pages": "~2.1.1",
        "identity-obj-proxy": "~3.0.0",
        "jest": "~24.9.0",
        "node-sass": "~4.12.0",
        "rollup": "~1.23.1",
        "rollup-plugin-babel": "~4.3.3",
        "rollup-plugin-commonjs": "~10.1.0",
        "rollup-plugin-node-resolve": "~5.2.0",
        "rollup-plugin-peer-deps-external": "~2.2.0",
        "rollup-plugin-postcss": "~2.0.3",
        "rollup-plugin-terser": "~5.1.2",
        "sass-loader": "~8.0.0",
        "style-loader": "~1.0.0",
        "stylelint": "~11.0.0",
        "stylelint-config-recommended-scss": "~4.0.0",
        "stylelint-scss": "~3.11.1"
    },
    "dependencies": {
        "classnames": "^2.2.6",
        "lodash.debounce": "^4.0.8",
        "lodash.escaperegexp": "^4.1.2",
        "lodash.isequal": "^4.5.0",
        "memoize-one": "^5.1.1"
    },
    "peerDependencies": {
        "prop-types": "^15.7.2",
        "react": "^16.8.3",
        "react-dom": "^16.8.3"
    },
    "jest": {
        "testEnvironment": "jsdom",
        "setupFilesAfterEnv": [
            "<rootDir>/test/setupTests.js"
        ],
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "moduleNameMapper": {
            "\\.(scss)$": "identity-obj-proxy"
        },
        "clearMocks": true
    },
    "bugs": {
        "url": "https://github.com/CarstenWickner/react-jsonschema-inspector/issues"
    },
    "keywords": [
        "react",
        "jsonschema",
        "schema",
        "viewer",
        "inspector"
    ]
}

{
    "name": "react-jsonschema-inspector",
    "version": "2.0.0",
    "description": "View component for traversing/searching in a JSON Schema",
    "homepage": "https://CarstenWickner.github.io/react-jsonschema-inspector",
    "author": "Carsten Wickner",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/CarstenWickner/react-jsonschema-inspector.git"
    },
    "scripts": {
        "start": "start-storybook -p 9001 -c .storybook",
        "build": "webpack",
        "lint": "eslint src stories test",
        "test": "jest",
        "predeploy": "npm run lint && npm run test --bail && build-storybook -c .storybook -o docs",
        "deploy": "gh-pages -d docs",
        "prepublishOnly": "npm run lint && npm run test --bail && npm run build"
    },
    "main": "build/index.js",
    "files": [
        "/build"
    ],
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.3.4",
        "@babel/plugin-proposal-class-properties": "^7.3.4",
        "@babel/preset-env": "^7.3.4",
        "@babel/preset-react": "^7.0.0",
        "@storybook/addon-actions": "^5.0.0",
        "@storybook/addon-info": "^5.0.0",
        "@storybook/addon-knobs": "^5.0.0",
        "@storybook/addon-options": "^5.0.0",
        "@storybook/react": "^5.0.0",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^24.1.0",
        "babel-loader": "^8.0.5",
        "coveralls": "^3.0.3",
        "css-loader": "^2.1.0",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.10.0",
        "enzyme-to-json": "^3.3.5",
        "eslint": "^5.15.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-plugin-babel": "^5.3.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-react": "^7.12.4",
        "gh-pages": "^2.0.1",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^24.1.0",
        "node-sass": "^4.11.0",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "webpack-cli": "^3.2.3"
    },
    "dependencies": {
        "classnames": "^2.2.6",
        "lodash.debounce": "^4.0.8",
        "lodash.escaperegexp": "^4.1.2",
        "lodash.isequal": "^4.5.0",
        "memoize-one": "^5.0.0"
    },
    "peerDependencies": {
        "prop-types": "^15.7.2",
        "react": "^16.8.3",
        "react-dom": "^16.8.3"
    },
    "jest": {
        "testEnvironment": "jsdom",
        "setupFilesAfterEnv": [
            "<rootDir>/test/setupTests.js"
        ],
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "moduleNameMapper": {
            "\\.(scss)$": "identity-obj-proxy"
        }
    },
    "bugs": {
        "url": "https://github.com/CarstenWickner/react-jsonschema-inspector/issues"
    },
    "keywords": [
        "react",
        "jsonschema",
        "schema",
        "viewer",
        "inspector"
    ]
}
